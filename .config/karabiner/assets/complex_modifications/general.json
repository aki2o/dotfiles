{
  "title": "General",
  "rules": [
    {
      "description": "My emacs moving on other than emacs",
      "manipulators": [
        {
          "type": "basic",
          "from": { "key_code": "j", "modifiers": { "mandatory": ["control"] } },
          "to": [{ "key_code": "down_arrow" }],
          "conditions": [{ "type": "frontmost_application_unless", "bundle_identifiers": ["^org\\.gnu\\.Emacs"] }]
        },
        {
          "type": "basic",
          "from": { "key_code": "k", "modifiers": { "mandatory": ["control"] } },
          "to": [{ "key_code": "up_arrow" }],
          "conditions": [{ "type": "frontmost_application_unless", "bundle_identifiers": ["^org\\.gnu\\.Emacs"] }]
        },
        {
          "type": "basic",
          "from": { "key_code": "h", "modifiers": { "mandatory": ["control"] } },
          "to": [{ "key_code": "left_arrow" }],
          "conditions": [{ "type": "frontmost_application_unless", "bundle_identifiers": ["^org\\.gnu\\.Emacs"] }]
        },
        {
          "type": "basic",
          "from": { "key_code": "l", "modifiers": { "mandatory": ["control"] } },
          "to": [{ "key_code": "right_arrow" }],
          "conditions": [{ "type": "frontmost_application_unless", "bundle_identifiers": ["^org\\.gnu\\.Emacs"] }]
        },
        {
          "type": "basic",
          "from": { "key_code": "j", "modifiers": { "mandatory": ["control", "shift"] } },
          "to": [{ "key_code": "down_arrow", "modifiers": ["fn"] }],
          "conditions": [{ "type": "frontmost_application_unless", "bundle_identifiers": ["^org\\.gnu\\.Emacs"] }]
        },
        {
          "type": "basic",
          "from": { "key_code": "k", "modifiers": { "mandatory": ["control", "shift"] } },
          "to": [{ "key_code": "up_arrow", "modifiers": ["fn"] }],
          "conditions": [{ "type": "frontmost_application_unless", "bundle_identifiers": ["^org\\.gnu\\.Emacs"] }]
        }
      ]
    },
    {
      "description": "My emacs editing on other than emacs",
      "manipulators": [
        {
          "type": "basic",
          "from": { "key_code": "g", "modifiers": { "mandatory": ["control"] } },
          "to": [{ "key_code": "escape" }],
          "conditions": [{ "type": "frontmost_application_unless", "bundle_identifiers": ["^org\\.gnu\\.Emacs"] }]
        },
        {
          "type": "basic",
          "from": { "key_code": "z", "modifiers": { "mandatory": ["control"] } },
          "to": [{ "key_code": "escape" }],
          "conditions": [{ "type": "frontmost_application_unless", "bundle_identifiers": ["^org\\.gnu\\.Emacs"] }]
        },
        {
          "type": "basic",
          "from": { "key_code": "m", "modifiers": { "mandatory": ["control"] } },
          "to": [{ "key_code": "return_or_enter" }],
          "conditions": [{ "type": "frontmost_application_unless", "bundle_identifiers": ["^org\\.gnu\\.Emacs"] }]
        },
        {
          "type": "basic",
          "from": { "key_code": "i", "modifiers": { "mandatory": ["control"] } },
          "to": [{ "key_code": "tab" }],
          "conditions": [{ "type": "frontmost_application_unless", "bundle_identifiers": ["^org\\.gnu\\.Emacs"] }]
        },
        {
          "type": "basic",
          "from": { "key_code": "s", "modifiers": { "mandatory": ["control"] } },
          "to": [{ "key_code": "f", "modifiers": ["command"] }],
          "conditions": [{ "type": "frontmost_application_unless", "bundle_identifiers": ["^org\\.gnu\\.Emacs"] }]
        }
      ]
    }
  ]
}
